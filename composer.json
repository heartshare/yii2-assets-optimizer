{
    "name": "slinstj/yii2-assets-optimizer",
    "description": "Unifies and compresses js and css files in your AssetBundles. Uses internal server cache to speed up all the process, reducing the time wasting.",
    "type": "yii2-extension",
    "keywords": ["yii2","minify","combine","assets"],
    "homepage": "https://github.com/slinstj/yii2-assets-optimizer",
    "license": "MIT",
    "authors": [
        {
            "name": "Sidney Lins",
            "email": "slinstj@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "2.0.*",
        "matthiasmullie/minify": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "4.7.*"
    },
    "autoload": {
        "psr-4": {
            "slinstj\\AssetsOptimizer\\": ""
        }
    }
}
